local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local KeyWindow = OrionLib:MakeWindow({Name = "Key System", HidePremium = false, IntroEnabled = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Players = game.Players.LocalPlayer
_G.Key = "L11fouBLj9"
_G.KeyInput = "string"

function CheckKey()
    if _G.Key == _G.KeyInput then
        OrionLib:MakeNotification({
            Name = "Key Correct",
            Content = "Key Correct! Welcome Back "..Players.Name.."!",
            Image = "rbxassetid://4483345998",
            Time = 2
        })

        wait(2)

        OrionLib:Destroy()

    else
        OrionLib:MakeNotification({
            Name = "Key Inorrect",
            Content = "Key Incorrect, Please Get The Correct Key",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
end

OrionLib:MakeNotification({
	Name = "Logged In!",
	Content = "Logged In As "..Players.Name..".",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local KeyTab = KeyWindow:MakeTab({
	Name = "Key Tab",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

KeyTab:AddTextbox({
	Name = "Enter Key Here",
	Default = "Key",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

KeyTab:AddButton({
	Name = "Check Key",
	Callback = function()
      	CheckKey()
  	end    
})

KeyTab:AddButton({
	Name = "Get Key",
	Callback = function()
        setclipboard("https://discord.gg/yu9tKsfuA8")
        OrionLib:MakeNotification({
            Name = "Copied To Clipboard",
            Content = "Discord Invite Link Copied To Clipboard",
            Image = "rbxassetid://4483345998",
            Time = 10
        })
  	end    
})

OrionLib:Init()
